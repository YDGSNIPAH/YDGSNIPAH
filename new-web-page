                return __getSession('membership') === 'gold' ||
                    noAdPages.filter(function(page){ return location.pathname.match(page); }).length !== 0 ||
                    !__getSession('sub')
            })(),
            AD_UNITS: {},
            SPLIT_TEST_ID: parseInt(__getCookie('st-id')),
            CACHE_ID: "90f8a46eb8e9cb30cfe8025862b7332d5e1f5d30",
